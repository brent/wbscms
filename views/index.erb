<div id="wrapper">
  <div class="header">
    <div class="inner">
       <h1 class="logo"></h1>
    </div><!-- END .inner -->
  </div><!-- END .header -->

  <% @posts.each do |post| %>

  <div class="post">
    <div class="inner">
      <h2><a href="<%= post[:path] %>"><%= post[:title].capitalize %></a></h2>
      <h3 class="date">{{ post.date }}</h3>
    </div>
  </div><!-- END .post -->

  <% end %>

</div><!-- END #wrapper -->
